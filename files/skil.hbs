    <style>
        body {
            background-image: linear-gradient(to left, lightblue, lightgrey, lightgrey);
        }

        .aaa {
            border: 1px solid black;
            border-radius: 20px;
            display: flex;
            justify-content: center;
            align-items: center;
            flex-direction: column;
            display: inline-block;

        }

        .bbb {
            border: 1px solid black;
            height: 20px;
            width: 200px;
            display: flex;
            justify-content: start;
            align-items: center;
            margin: 10px;
            border-radius: 20px;
            overflow: hidden;
            margin: 20px;
        }

        #ab {
            background-color: lightblue;
            width: 170px;
            ;
        }

        #bb {
