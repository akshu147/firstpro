            width: 160px;

        }

        b {
            margin-left: 10px;
        }

        #ac {
            background-color: lightblue;
            width: 120px;
        }

        .menbox {
            position: absolute;
            border: 3px solid black;
            border-radius: 10px 30px 10px 30px;
            left: 100px;
            top: 150px;
            position: fixed;
            padding: 20px;
            outline: 3px solid black;
            outline-offset: 2px;



        }

        #ba {
            background-color: lightblue;
            width: 140px;
        }

        #bc {
